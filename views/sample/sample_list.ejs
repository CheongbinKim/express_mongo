<!-- Begin Page Content -->
<div class="container-fluid">
        <!-- DataTales Example -->
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">List</h6>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Date</th>
                                <th>Title</th>
                                <th>Content</th>
                                <th>Delete</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let data of datas){%>
                            <tr>
                                <td nowrap><%=moment(data.created_at).format("YYYY-MM-DD")%></td>
                                <td nowrap><%=data.title%></td>
                                <td nowrap><%=data.content%></td>
                                <td nowrap><button class="btn btn-danger btn-circle k-remove" data-id=<%=data._id%>><i
                                            class="fas fa-trash"></i></button></td>
                            </tr>
                            <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div> 
    </div>
    <!-- /.container-fluid -->
            